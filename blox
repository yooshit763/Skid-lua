	pcall(function()
		while wait(.1) do
			if PressHomeStopTween then
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Home) then
					TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				end
			end
		end
	end)
end)

spawn(function()
    game:GetService("RunService").Heartbeat:Connect(function()
        if FarmMasteryFruit or FarmMasteryGun or Farm_Level or AutoFarmSelectMonster or AutoFarmNearest or Auto_Bone or AutoBossNoQuest or AutoBossQuest then
            for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                if v.Name and v:FindFirstChild("Humanoid") then
                    v.Stun.Value = 1
                end
            end
        end
    end)
end)

spawn(function()
    game:GetService("RunService").Heartbeat:Connect(function()
        if FarmMasteryFruit or FarmMasteryGun or Farm_Level or Clip or noclip or Noclip or AutoFarmSelectMonster or AutoFarmNearest or _G.Bounty1 or Auto_Bone or AutoFarmBoss or FarmMastery or KillPlayer or AutoFarmObservation or _G.Superhuman or _G.DeathStep or _G.AutoElectricClawV2 or _G.AutoPlayerHunter or _G.Pole or _G.PoleHop or _G.AutoNew or _G.AutoSaber or _G.OpenSwan or sea3 or sea33 or _G.AutoaRengoku or _G.Factory or _G.SwanGlasses or _G.SwanHop or _G.BlackBeardHop or _G.EliteHunt or _G.ElitehuntHop or rainbowhaki or _G.YamaHop or _G.Tushitahop or _G.Cave or _G.HolyTorch or AutoBartilo or AutoEvoRace or AutoEcto or AutoCitizen or AutoBossNoQuest or AutoBossQuest then
            if not game:GetService("Workspace"):FindFirstChild("LOL") then
                local LOL = Instance.new("Part")
                LOL.Name = "LOL"
                LOL.Parent = game.Workspace
                LOL.Anchored = true
                LOL.CanCollide = false
                LOL.Transparency = 1
                LOL.Size = Vector3.new(30,1.5,30)
                LOL.Material = "SmoothPlastic"
            elseif game:GetService("Workspace"):FindFirstChild("LOL") then
                game.Workspace["LOL"].CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-3.6,0)
            end
        else
            if game:GetService("Workspace"):FindFirstChild("LOL") then
                game:GetService("Workspace"):FindFirstChild("LOL"):Destroy()
            end
        end
    end)
end)

spawn(function()
    player = game.Players.LocalPlayer
    character = player.Character
    game:GetService("RunService").Stepped:Connect(function()
        if FarmMasteryFruit or FarmMasteryGun or Farm_Level or Clip or noclip or Noclip or AutoFarmSelectMonster or AutoFarmNearest or _G.Bounty1 or Auto_Bone or AutoFarmBoss or FarmMastery or KillPlayer or AutoFarmObservation or _G.Superhuman or _G.DeathStep or _G.AutoElectricClawV2 or _G.AutoPlayerHunter or _G.Pole or _G.PoleHop or _G.AutoNew or _G.AutoSaber or _G.OpenSwan or sea3 or sea33 or _G.AutoaRengoku or _G.Factory or _G.SwanGlasses or _G.SwanHop or _G.BlackBeardHop or _G.EliteHunt or _G.ElitehuntHop or rainbowhaki or _G.YamaHop or _G.Tushitahop or _G.Cave or _G.HolyTorch or AutoBartilo or AutoEvoRace or AutoEcto or AutoCitizen or AutoBossNoQuest or AutoBossQuest then
            for _, v in pairs(character:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.CanCollide = false
                    Workspace.Gravity = 0
                end
            end
        else
            Workspace.Gravity = 196
        end
    end)
end)

