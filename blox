-- Tab />
local Farm_Level = window:Tab("Farm")
-----------------------------------------------------------------------------------
-- Farm_Level
Farm_Level:TimeSet()

Farm_Level:Label("- Quest") 
Wapon = {}
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
	if v:IsA("Tool") then
	   table.insert(Wapon ,v.Name)
	end
end
for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do  
	if v:IsA("Tool") then
	   table.insert(Wapon, v.Name)
	end
end

local SelectWeapon = Farm_Level:Dropdown("Select Weapon",Wapon,function(Value)
    SelectToolWeapon = Value
    SelectToolWeaponOld = Value
end)

Farm_Level:Toggle("[Quest] Farm Level", false, function(vu)
    _G.AutoFarm = vu
	if _G.AutoFarm and SelectToolWeapon == "" then
		print(weapon) 
	else
		Auto_Farm = vu
		SelectMonster = ""
	end
end)
    
Farm_Level:Toggle("Farm Bone [Third Sea]", false, function(vu)
	Auto_Bone = vu
end)

Farm_Level:Label("- Settings") 
-----------------------------------------------------------------------------------
-- Farm_Settings
Farm_Level:Slider("Farm Distance", 0, 50, Y, function(val)
    Y = val
end)

Farm_Level:Toggle("Auto Spawn Point", true, function(vu)
    AutoSetSpawn = vu
end)
spawn(function()
    while wait() do
        if AutoSetSpawn then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
        end
    end
end)

Farm_Level:Toggle("Auto Click Weapon", false, function(vu)
    _G.FastAtttk = vu
end)

Farm_Level:Toggle("Bring Mob", true, function(vu)
    Magnet = vu
end)

Farm_Level:Toggle("Invisible Mob", false, function(value)
    _G.inv = value
    while wait() do
         if _G.inv then
            pcall(function()
                for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
                    if v.ClassName == "MeshPart" then
                        v.Transparency = 1
                    end
                end
                for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
                    if v.Name == "Head" then
                        v.Transparency = 1
                    end
                end
                for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
                    if v.ClassName == "Accessory" then
                        v.Handle.Transparency = 1
                    end
                end
                for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
                    if v.ClassName == "Decal" then
                        v.Transparency = 1
                    end
                end
            end)
        end
    end
end)

Farm_Level:Toggle("Auto Activated Buso Haki", false, function(vu)
    Auto_Haki = vu
end)

Farm_Level:Toggle("Auto Activated Observation Haki", false, function(vu)
	AutoObservation = vu
end)

